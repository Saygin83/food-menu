#include <stdio.h>
int main() 
{
    int x;
    printf("Order please!\n 1.Pizza\n 2.Burger\n 3.Pasta\n 4.French Fries\n 5.Sandwich\n");
    scanf("%d",&x);
switch(x)
{
    case 1:
       printf("Food item- Pizza\n price-239/-");
       break;
    case 2:
       printf("Food item- Burger\n Price-129/-");
       break;
    case 3:
       printf("Food item- Pasta\n Price-179/-");
       break;
    case 4:
       printf("Food item- French Fries\n Price-99/-");
       break;
    case 5:
       printf("Food item- Sandwich\n Price-149");
       break;
    default:
      printf("change the order up please!");
    return 0;
}   
}
